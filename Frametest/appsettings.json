{
  "Serilog": {
    "MinimumLevel": "Debug",
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/test.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Overrides": {
      "Microsoft": "Warning"
    }
  },
  "DefaultNameSpace": "Frameset.Test",
  "AllowedHosts": "*"
}